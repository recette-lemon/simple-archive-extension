{
	"name": "archive-buddy",
	"version": "1.0",
	"description": "makes archiving and searching archives easier",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon/16.png",
			"32": "icon/32.png",
			"48": "icon/48.png",
			"128": "icon/128.png"
		}
	},
	"icons": {
		"16": "icon/16.png",
		"32": "icon/32.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	},
	"permissions": [
		"activeTab"
	]
}